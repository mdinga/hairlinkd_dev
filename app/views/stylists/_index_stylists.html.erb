      <div class = 'card-header'>
        <h5 class = 'card-title'><%= stylist.name %></h5>
      </div>

      <div class="img-thumbnail rounded card-img">
        <%= image_tag stylist.avatar.url(:thumb).to_s, :style => "width:150px;"%>
      </div>

      <p class = 'card-text'> <strong> Rating:</strong> <%=  stylist.overall_rating %> (<%= pluralize(stylist.ratings.count, 'Reviews') %>) <br />
       <strong>City:</strong>
       <% stylist.cities.each do |c|  %>
       <%= c.city %>
        <% end %></p>
