<h2>City Names</h2>
<%= link_to("Create City", new_city_path)%>
<hr>

<div class = "container">
  <table class = 'table table-hover'>

    <tr>
      <th>City</th>
      <th>Action </th>
    </tr>

    <% @city.each do |c| %>
    <tr>
      <td><%= c.city %></td>

      <td>
          <%= link_to("Edit", edit_city_path(c))%>
          <%= link_to("Delete", city_path(c), method: :delete)%>
      </td>
    </tr>
    <% end %>
  </table>

</div><!--container-->
