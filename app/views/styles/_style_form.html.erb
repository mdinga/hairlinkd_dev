<table>

  <tr>
    <th>Style Picture</th>
    <td><%= s.file_field(:image) %></td>
  </tr>

  <tr>
    <th>Hairstyle</th>
    <td><%= s.select(:service_id, @service.map{|h| [h.name, h.id]}) %></td>
  </tr>

  <tr>
    <th>Details</th>
    <td><%= s.text_area(:details, :size => "50x5") %></td>
  </tr>

</table>
